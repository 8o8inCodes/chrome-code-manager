{
    "name": "Chrome Code Injector",
    "version": "1.0",
    "description": "Injects code into chrome",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage",
        "tabs",
        "http://*/",
        "https://*/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "content_security_policy": "script-src 'self' 'sha256-aiTCej56IR640CN++qOoP5HtiJlWlKhUOqwYWIkmsOE='; object-src 'self'",
    "manifest_version": 2
}